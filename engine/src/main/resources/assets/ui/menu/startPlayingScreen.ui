{
  "type": "engine:StartPlayingScreen",
  "skin": "engine:mainMenu",
  "contents": {
    "type": "relativeLayout",
    "contents": [
      {
        "type": "UILabel",
        "id": "title",
        "family": "title",
        "text": "${engine:menu#start-playing}",
        "layoutInfo": {
          "height": 48,
          "position-horizontal-center": {},
          "position-top": {
            "target": "TOP",
            "offset": 48
          }
        }
      },
      {
        "type": "UIBox",
        "id": "container",
        "layoutInfo": {
          "width": 400,
          "position-horizontal-center": {},
          "position-top": {
            "target": "BOTTOM",
            "offset": 16,
            "widget": "title"
          },
          "position-bottom": {
            "target": "TOP",
            "widget": "actionsRow",
            "offset": 24
          }
        }
      },
      {
        "type": "ColumnLayout",
        "columns": 1,
        "horizontalSpacing": 8,
        "layoutInfo": {
          "width": 384,
          "position-horizontal-center": {},
          "position-top": {
            "target": "TOP",
            "widget": "container",
            "offset": 8
          },
          "position-bottom": {
            "target": "TOP",
            "widget": "actionsRow",
            "offset": 32
          }
        },
        "contents": [
          {
            "type": "UIImage",
            "skin": "framed_image",
            "id": "preview"
          }
        ]
      },
      {
        "type": "RowLayout",
        "id": "actionsRow",
        "horizontalSpacing": 80,
        "contents": [
          {
            "type": "RelativeLayout",
            "contents": [
              {
                "type": "UILabel",
                "text": "Previous: Pregeneration",
                "layoutInfo": {
                  "use-content-height": true,
                  "position-bottom": {
                    "target": "TOP",
                    "widget": "close"
                  }
                }
              },
              {
                "type": "UIButton",
                "text": "${engine:menu#back}",
                "id": "close",
                "layoutInfo": {
                  "position-vertical-center": {}
                }
              },
              {
                "type": "UILabel",
                "text": "Earlier: Universe Setup, Module Select",
                "layoutInfo": {
                  "use-content-height": true,
                  "position-top": {
                    "target": "BOTTOM",
                    "widget": "close"
                  }
                }
              }
            ],
            "layoutInfo": {
              "width": 180,
              "height": 120
            }
          },
          {
            "type": "UIButton",
            "text": "${engine:menu#start-game}",
            "id": "play"
          }
        ],
        "layoutInfo": {
          "width": 440,
          "height": 120,
          "position-horizontal-center": {},
          "position-bottom": {
            "target": "BOTTOM",
            "offset": 48
          }
        }
      }
    ]
  }
}
